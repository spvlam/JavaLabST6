<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AimsProject.src.hust.soict.globalict.aims.screen.controller.ItemController">
    <children>
        <VBox alignment="CENTER" prefHeight="175.0" prefWidth="305.0">
            <children>
                <Label fx:id="lblTitle" alignment="CENTER" prefHeight="50.0" prefWidth="305.0" text="Label" />
                <Label fx:id="lblCost" alignment="CENTER" prefHeight="50.0" prefWidth="305.0" text="Label" />
                <HBox alignment="CENTER" prefHeight="50.0" prefWidth="305.0" spacing="20.0">
                    <children>
                        <Button fx:id="btnAddToCart" mnemonicParsing="false" onAction="#btnAddToCartClicked" text="Add to Cart" />
                        <Button fx:id="btnPlay" mnemonicParsing="false" onAction="#btnPlayClicked" text="Play" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>